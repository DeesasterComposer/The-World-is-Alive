  "entrypoints": {
    "main": [
      "net.deesaster.theworldisalive.TheWorldIsAlive"
    ]
  },
  "mixins": [
    "theworldisalive.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.x",
